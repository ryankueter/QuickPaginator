<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<TargetFrameworks>net6.0;net7.0;net8.0;net9.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
		<Authors>Ryan A. Kueter</Authors>
		<Company>Ryan A. Kueter</Company>
		<Copyright>Ryan A. Kueter</Copyright>
		<Description>Quick Paginator is a free .NET library that provides a simple and robust way to paginate a list of items.</Description>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<Version>1.0.23.0</Version>
		<AssemblyVersion>1.0.23.0</AssemblyVersion>
		<FileVersion>1.0.23.0</FileVersion>
		<PackageReadmeFile>README.md</PackageReadmeFile>
		<RepositoryType>Git</RepositoryType>
		<RepositoryUrl>https://github.com/ryankueter/QuickPaginator</RepositoryUrl>
		<PackageTags>C#;Pager;Pagination</PackageTags>
		<Product>$(AssemblyName)</Product>
		<NeutralLanguage>en</NeutralLanguage>
		<Title>QuickPaginator</Title>
		<PackageProjectUrl>https://github.com/ryankueter/QuickPaginator</PackageProjectUrl>
		<PackageReleaseNotes>Minor bug fix.</PackageReleaseNotes>
	</PropertyGroup>

	<ItemGroup>
		<None Include="README.md">
			<Pack>True</Pack>
			<PackagePath>\</PackagePath>
		</None>
		<None Include="LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>
</Project>
